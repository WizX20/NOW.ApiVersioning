{
  "AllowedHosts": "*",
  "ApiVersioning": {
    "DefaultVersion": {
      "Major": 1,
      "Minor": 0
    },
    "ApiDescription": {
      "Title": "Test API",
      "Description": "",
      "Contact": {
        "Name": "Company Name",
        "Email": "info@company.com"
      },
      "License": {
        "Name": "MIT",
        "Url": "https://mit-license.org/"
      }
    }
  },
  "ApiTestApplication": {
    "CorsPolicy": {
      "AllowedOrigins": "*"
    },
    "Localization": {
      "DefaultCulture": "en-US",
      "SupportedCultures": "en-US"
    },
    "ResponseCaching": {
      "MaximumBodySize": 2048,
      "UseCaseSensitivePaths": false
    },
    "Swagger": {
      "JsonRoute": "swagger/{documentName}/swagger.json",
      "Description": "Test API",
      "UIPrefix": ""
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}